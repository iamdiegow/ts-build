{
  "name": "@diegow/ts-build",
  "description": "monorepo with examples of ts builds",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "turbo": "latest",
    "typescript": "^4.5.5"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "npm@8.1.0"
}
